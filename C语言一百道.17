#include <stdio.h>
/*
题目：利用条件运算符的嵌套来完成此题：学习成绩 >= 90分的同学用A表示，60 - 89分之间的用B表示，60分以下的用C表示。

*/

int main(int argc, int **argv){

	int sorce = 0;

	char grade = 'A';

	printf("请输入你的成绩：\n");

	scanf("%d", &sorce);

	grade = (sorce >= 90) ? 'A' : ((sorce >= 60) ? 'B' : 'C');

	printf("你的成绩等级：%c", grade);

	return 0;

}
